class Solution {
    public boolean isAnagram(String s, String t) {
        char[] str=s.toCharArray();
        char[] str1=t.toCharArray();
        Arrays.sort(str);
        Arrays.sort(str1);
        String a=new String(str);
        String b=new String(str1);
        //System.out.print(a+"  "+b);
        if(a.equals(b))
            return true;
            
        return false; 
        
    }
}
